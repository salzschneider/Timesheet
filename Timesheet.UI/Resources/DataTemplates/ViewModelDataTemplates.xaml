<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewmodels="clr-namespace:Timesheet.UI.ViewModels"
                    xmlns:views="clr-namespace:Timesheet.UI.Views">

    <DataTemplate DataType="{x:Type viewmodels:StartViewModel}">
        <views:StartView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:ActivityMgmtViewModel}">
        <views:ActivityMgmtView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:UserMgmtViewModel}">
        <views:UserMgmtView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:WorklogViewModel}">
        <views:WorklogView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:ReportViewModel}">
        <views:ReportView/>
    </DataTemplate>

</ResourceDictionary>